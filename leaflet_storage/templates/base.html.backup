{% extends "site_base.html" %}
<head>
    <title>{% block title %}Leaflet Storage Map{% endblock %}</title>
    {% block extra_head %}
    {% endblock %}
</head>
<body {% block body_id %}{% endblock %} {% block body_class %}{% endblock %}>
    {% block extrabody %}
    {% endblock %}
    {% block body %}
        <div id="header">
            {% block header %}
            {{ block.super }}
            {% endblock %}
        </div>
        <div id="sidebar">
            {% block sidebar %}
            {{ block.super }}
            {% endblock %}
        </div>
        <div id="content">
            {% block content %}
            {{ block.super }}
            {% endblock %}
        </div>
        <div id="footer">
            {% block footer %}
            {{ block.super }}
            {% endblock %}
        </div>
    {% endblock %}
</body>
